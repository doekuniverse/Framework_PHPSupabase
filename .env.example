# Configuración de Supabase
# Reemplaza estos valores con tus propias credenciales de Supabase
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_KEY=tu-clave-anon-publica

# Configuración de la aplicación
# Puedes personalizar estos valores según tus necesidades
APP_NAME=Mi Aplicación con Supabase
APP_URL=http://localhost
APP_DEBUG=true

# Configuración de cookies
# Recomendado: mantener estos valores para seguridad
COOKIE_SECURE=true
COOKIE_HTTPONLY=true
COOKIE_SAMESITE=Strict
COOKIE_LIFETIME=3600

# Configuración de seguridad adicional
# Puedes personalizar estos valores según tus necesidades
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60
